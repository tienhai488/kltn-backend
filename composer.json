{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "dedoc/scramble": "^0.11.12",
        "fingerprint/fingerprint-pro-server-api-sdk": "^5.1",
        "guzzlehttp/guzzle": "^7.2",
        "khgc/khgc-export-excel": "dev-master",
        "khgc/khgc-import-excel": "dev-master",
        "laravel/framework": "^11.0",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "livewire/livewire": "^3.1",
        "maatwebsite/excel": "^3.1",
        "owen-it/laravel-auditing": "^13.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-medialibrary": "^11.0.0",
        "spatie/laravel-permission": "^6.0",
        "zaloplatform/zalo-php-sdk": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "^3.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/envoy": "^2.8",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:khgc-bo/khgc-export-excel.git"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:khgc-bo/khgc-import-excel.git"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
